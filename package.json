{
  "name": "nest-by-eden-oasis",
  "version": "0.1.0",
  "private": true,
  "description": "Nest by Eden Oasis - Comprehensive Asset Management System",
  "keywords": [
    "asset management",
    "equipment tracking",
    "inventory",
    "nest",
    "eden oasis"
  ],
  "author": "Eden Oasis",
  "scripts": {
    "dev": "next dev --turbopack -p 9002",
    "build": "next build",
    "analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "generate-icons": "node scripts/generate-pwa-icons.js",
    "test": "jest",
    "validate-env": "node -e \"console.log('Environment validation passed'); require('./src/lib/env-validation').validateEnvironment();\"",
    "test-db": "node -e \"console.log('Testing database connection...'); require('./src/lib/supabase/client-server').createClient();\"",
    "test-email": "node -e \"console.log('Testing email configuration...'); require('./src/lib/email').testEmailConfig();\"",
    "realtime:check": "tsx src/utils/supabase-realtime-checker.ts",
    "realtime:setup": "node scripts/db/enable-realtime.js",
    "troubleshoot:realtime": "node scripts/troubleshooting/check-realtime-tables.js",
    "add-timestamps": "node scripts/db/add-timestamp-columns.js",
    "cleanup": "node scripts/cleanup-codebase.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@lottiefiles/dotlottie-react": "^0.8.0",
    "@supabase/supabase-js": "^2.38.4",
    "@supabase/ssr": "^0.0.10",
    "bcryptjs": "^2.4.3",
    "date-fns": "^3.3.1",
    "framer-motion": "^11.0.3",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.302.0",
    "next": "^14.0.0",
    "node-cron": "^3.0.3",
    "node-fetch": "^2.6.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "resend": "^3.2.0",
    "web-push": "^3.6.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "typescript": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-config-next": "^14.0.0",
    "tailwindcss": "^3.0.0",
    "autoprefixer": "^10.0.0",
    "postcss": "^8.0.0",
    "jest": "^29.0.0",
    "tsx": "^4.0.0"
  }
}